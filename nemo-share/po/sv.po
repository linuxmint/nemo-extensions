#: ../src/nemo-share.c:128
#, c-format
msgid "Nemo needs to add some permissions to your folder \"%s\" in order to share it"
msgstr "Nemo behöver lägga till några rättigheter till din mapp \"%s\" för att kunna dela ut den"

#: ../src/nemo-share.c:136
#, c-format
msgid ""
"The folder \"%s\" needs the following extra permissions for sharing to work:\n"
"%s%s%sDo you want Nemo to add these permissions to the folder automatically?"
msgstr ""
"Mappen \"%s\" behöver följande extra rättigheter för att utdelningen ska fungera:\n"
"%s%s%sVill du att Nemo automatiskt ska lägga till dessa rättigheter till mappen?"

#: ../src/nemo-share.c:140
msgid "  - read permission by others\n"
msgstr "  - läsrättighet för andra\n"

#: ../src/nemo-share.c:141
msgid "  - write permission by others\n"
msgstr "  - skrivrättighet för andra\n"

#: ../src/nemo-share.c:142
msgid "  - execute permission by others\n"
msgstr "  - körrättighet för andra\n"

#: ../src/nemo-share.c:146
msgid "Add the permissions automatically"
msgstr "Lägg till rättigheterna automatiskt"

#: ../src/nemo-share.c:172
#, c-format
msgid "Could not change the permissions of folder \"%s\""
msgstr "Kunde inte ändra rättigheterna för mappen \"%s\""

#: ../src/nemo-share.c:451
msgid "Share name is too long"
msgstr "Utdelningsnamnet är för långt"

#: ../src/nemo-share.c:485
msgid "The share name cannot be empty"
msgstr "Utdelningsnamnet får inte vara blankt"

#: ../src/nemo-share.c:498
#, c-format
msgid "Error while getting share information: %s"
msgstr "Fel vid hämtning av utdelningsinformation: %s"

#: ../src/nemo-share.c:508
msgid "Another share has the same name"
msgstr "En annan utdelning har samma namn"

#: ../src/nemo-share.c:555
#: ../src/nemo-share.c:790
msgid "Modify _Share"
msgstr "Ändra _utdelning"

#: ../src/nemo-share.c:555
#: ../src/nemo-share.c:792
msgid "Create _Share"
msgstr "Skapa _utdelning"

#: ../src/nemo-share.c:683
msgid "There was an error while getting the sharing information"
msgstr "Det inträffade ett fel med hämtning av utdelningsinformation"

#: ../src/nemo-share.c:1054
msgid "Share"
msgstr "Utdelning"

#: ../src/nemo-share.c:1114
msgid "Folder Sharing"
msgstr "Mapputdelning"

#: ../src/nemo-share.c:1155
msgid "Sharing Options"
msgstr "Utdelningsalternativ"

#: ../src/nemo-share.c:1156
msgid "Share this Folder"
msgstr "Dela ut den här mappen"

